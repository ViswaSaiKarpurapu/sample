<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample">
        <activity
            android:name=".JaipurActivity"
            android:label="@string/title_activity_jaipur"
            android:theme="@style/Theme.Sample.NoActionBar"></activity>
        <activity
            android:name=".SrinagarActivity"
            android:label="@string/title_activity_srinagar"
            android:theme="@style/Theme.Sample.NoActionBar" />
        <activity android:name=".RecyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>